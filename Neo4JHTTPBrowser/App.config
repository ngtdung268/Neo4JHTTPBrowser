	</runtime>
	<log4net>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value=".\\Logs\\" />
			<encoding value="utf-8" />
			<appendToFile value="true" />
			<DatePattern value="yyyy\\\\MM\\\\dd'.log'" />
			<rollingStyle value="Date" />
			<param name="StaticLogFileName" value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %level %logger - %message%newline%exception" />
			</layout>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="Console" />
			<appender-ref ref="RollingFileAppender" />
		</root>
	</log4net>
	<userSettings>
		<Neo4JHTTPBrowser.Properties.Settings>
   <setting name="Neo4JBaseUrl" serializeAs="String">
    <value>http://localhost:7474</value>
   </setting>
   <setting name="Neo4JVerifySsl" serializeAs="String">
    <value>False</value>
   </setting>
   <setting name="Neo4JSavedUrls" serializeAs="Xml">
    <value>
     <ArrayOfString xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <string>http://localhost:7474</string>
     </ArrayOfString>
    </value>
   </setting>
  </Neo4JHTTPBrowser.Properties.Settings>
